                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Storyboard x:Key="DashOffsetStoryboard" RepeatBehavior="Forever">
        <DoubleAnimation Storyboard.TargetProperty="BorderDashOffset"
                     From="0" To="8"
                     Duration="0:0:1"/>
    </Storyboard>

</ResourceDictionary>